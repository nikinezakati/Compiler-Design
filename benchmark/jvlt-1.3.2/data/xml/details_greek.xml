<?xml version="1.0"?>
<schema lang="greek">
	<class name="noun"/>
	<class name="verb"/>
	<class name="adverb"/>
	<class name="particle"/>
	<class name="adjective"/>
	<class name="conjunction"/>
	<class name="preposition"/>
	<class name="expression"/>
	<class name="interjection"/>
	<attribute name="gender" classes="noun">
		<choice name="feminine"/>
		<choice name="masculine"/>
		<choice name="neuter"/>
	</attribute>
	<attribute name="declcl" classes="noun" group="declension">
		<choice name="class01"/>
		<choice name="class02"/>
		<choice name="class03"/>
	</attribute>
	<attribute name="paradigm" classes="noun;verb;adjective"/>
</schema>

